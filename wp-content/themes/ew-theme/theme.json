{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "typography": {
      "customFontSize": false,
      "letterSpacing": true,
      "lineHeight": true,
      "fontFamilies": [
        {
          "name": "Primary",
          "slug": "primary",
          "fontFamily": "'Raleway', sans-serif",
          "fontFace": [
            {
              "fontFamily": "Raleway",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Raleway/Raleway-Regular.woff2"
              ]
            },
            {
              "fontFamily": "Raleway",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Raleway/Raleway-Bold.woff2"
              ]
            },
            {
              "fontFamily": "Raleway",
              "fontWeight": "400",
              "fontStyle": "italic",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Raleway/Raleway-Italic.woff2"
              ]
            },
            {
              "fontFamily": "Raleway",
              "fontWeight": "700",
              "fontStyle": "italic",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Raleway/Raleway-BoldItalic.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "size": "48px",
          "name": "Headline 1",
          "slug": "h1"
        },
        {
          "size": "40px",
